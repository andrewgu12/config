"*":
  "atom-clock":
    showClockIcon: true
  "atom-ide-ui":
    "atom-ide-code-format":
      formatOnType: false
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
        ".flowconfig"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "tree-view"
    ]
    telemetryConsent: "no"
    themes: [
      "city-lights-ui"
      "firewatch-syntax"
    ]
    titleBar: "custom"
  editor:
    fontFamily: "Source Code Pro"
    fontSize: 13
    preferredLineLength: 800
    showIndentGuide: true
  "exception-reporting":
    userId: "35af83ae-4998-434b-9d8d-47617c17f014"
  nuclide:
    "nuclide-home":
      showHome: false
  welcome:
    showOnStartup: false
".reason.source":
  editor:
    preferredLineLength: 110
